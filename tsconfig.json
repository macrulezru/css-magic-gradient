{
  "compilerOptions": {
    "target": "ES2018",
    "module": "NodeNext",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext"
  },
  "types": ["vue"],
  "include": ["src/**/*"]
}
